    crs-onto:hasCredits 3 ;
    crs-onto:hasDepartment "MANE"^^xsd:string ;
    crs-onto:hasDescription "The physical metallurgy and associated physical chemistry of problems encountered in the application of materials in nuclear reactors is discussed. Specifically, the metallurgy and physical chemistry of ceramic fuels (e.g., oxygen potentials), the primary fuel densification and pellet-clad interaction mechanisms, irradiation-induced creep, hardening, and embrittlement mechanisms, and the properties of zircalloy are covered."^^xsd:string ;
    crs-onto:hasLevel "6380"^^xsd:string ;
    crs-onto:hasName "Nuclear Reactor Materials"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs001364 ;
    crs-onto:hasTopic "placeholder for topic"^^xsd:string .

