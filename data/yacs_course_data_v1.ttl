    crs-onto:hasDepartment "MGMT"^^xsd:string ;
    crs-onto:hasDescription "Development of branding strategies to accomplish marketing objectives. The development of media plans and schedules to deliver advertising promotions element in the marketing mix. A student cannot receive credit for both the graduate and undergraduate versions of this course."^^xsd:string ;
    crs-onto:hasLevel "4490"^^xsd:string ;
    crs-onto:hasName "Advertising Strategy and Promotions"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs000769 ;
