    crs-onto:hasCourseCode "MANE-6480"^^xsd:string ;
    crs-onto:hasCredits 3 ;
    crs-onto:hasDepartment "MANE"^^xsd:string ;
    crs-onto:hasDescription "Use of radioisotopes and radiation in nuclear medicine, radiation chemistry, basis of dosimetry, ionizing and nonionizing energy transfer processes in living tissue and cells. Radiation effects on the structure of nucleic acids, proteins, and cell membranes with emphasis on mechanisms by which cell viability is lost. Background in radiation chemistry is developed in particular for engineering majors. Applications are given in nuclear medicine, cancer therapy, and radiation in the environment."^^xsd:string ;
    crs-onto:hasLevel "6480"^^xsd:string ;
    crs-onto:hasName "Health Physics and Medical Aspects of Radiation"^^xsd:string ;
    crs-onto:hasTopic "placeholder for topic"^^xsd:string .

