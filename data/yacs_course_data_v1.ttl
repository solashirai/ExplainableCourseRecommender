    crs-onto:hasDepartment "MANE"^^xsd:string ;
    crs-onto:hasDescription "Stress invariants. Polyaxial stress-strain relation for strain-hardening materials. Ideal plasticity, various yield conditions, and associated flow rules. Variational principles. Limit analysis. Applications in elastic-plastic stress analysis, metal forming, plastic collapse, and plastic instability."^^xsd:string ;
    crs-onto:hasLevel "6490"^^xsd:string ;
    crs-onto:hasName "Plasticity"^^xsd:string ;
    crs-onto:hasTopic "placeholder for topic"^^xsd:string .

