    crs-onto:hasDepartment "IHSS"^^xsd:string ;
    crs-onto:hasDescription "A communication-intensive course designed for students to develop their own voice as a songwriter. The course surveys the methods of successful songwriters, highlighting aspects of melody, lyrics, harmonic progression, story-telling, audience, and social context. Students develop a portfolio of their own original songs and lyrics, presented weekly and performed in a studio or live setting at the end of the term."^^xsd:string ;
    crs-onto:hasLevel "1700"^^xsd:string ;
    crs-onto:hasName "Songwriting Workshop"^^xsd:string ;
    crs-onto:hasTopic "placeholder for topic"^^xsd:string .

