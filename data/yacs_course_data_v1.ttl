crs-courses:crs000049 a crs-onto:Course ;
    crs-onto:hasCourseCode "ARCH-5110"^^xsd:string ;
    crs-onto:hasCredits 4 ;
    crs-onto:hasDepartment "ARCH"^^xsd:string ;
    crs-onto:hasDescription "This course addresses the history of architectural and related developments in selected Western and non-Western civilizations in order to construct a conceptual and strategic understanding of the relationships between architecture, culture, civilization, technology, and thought. In doing so, it will focus on key constructed spatial phenomena of the pre-modern world and, where relevant, elucidate the connection of these works of architecture and their motivating ideals to those of the modern and contemporary world."^^xsd:string ;
    crs-onto:hasLevel "5110"^^xsd:string ;
    crs-onto:hasName "History, Theory, Criticism 2"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs001016 ;
