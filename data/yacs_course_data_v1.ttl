    crs-onto:hasCredits 3 ;
    crs-onto:hasDepartment "MANE"^^xsd:string ;
    crs-onto:hasDescription "An introduction to the principles underlying the thermal-hydraulic design of nuclear power reactors. Topics include plant thermal limits, sub-channel analysis, thermal-hydraulic stability analysis, and reactor system response during both normal and postulated accident conditions."^^xsd:string ;
    crs-onto:hasLevel "6370"^^xsd:string ;
    crs-onto:hasName "Thermal-Hydraulic Design of Nuclear Reactors"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs001556 ;
    crs-onto:hasTopic "placeholder for topic"^^xsd:string .

