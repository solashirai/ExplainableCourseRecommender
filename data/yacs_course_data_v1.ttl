crs-courses:crs000919 a crs-onto:Course ;
    crs-onto:hasCourseCode "STSH-4250"^^xsd:string ;
    crs-onto:hasCredits 4 ;
    crs-onto:hasDepartment "STSH"^^xsd:string ;
    crs-onto:hasDescription "This course explores historical perspectives on bioethics through concrete cases and practical problems faced in the design and execution of some of the highest profile biomedical research and most consequential clinical decisions of the twentieth century. Topics include vaccine development; human radiation experiments; new genetic and reproductive technologies; right-to-die, death-with-dignity, and physician-assisted suicide; human experimentation, including prisoners, the sick, and the disabled; neuroethics; animal research; and emergent topics such as stem cell research, prenatal diagnostics, and genetic testing. This is a communication-intensive course."^^xsd:string ;
    crs-onto:hasLevel "4250"^^xsd:string ;
    crs-onto:hasName "Bioethics"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs000932 ;
