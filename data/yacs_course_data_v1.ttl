    crs-onto:hasDepartment "PSYC"^^xsd:string ;
    crs-onto:hasDescription "This course is an exploration of the social and psychological effects of extensive use of pharmacological agents that are salient to daily behavior. There is an emphasis on the effects of addictive drugs such as alcohol, heroin, and cocaine."^^xsd:string ;
    crs-onto:hasLevel "4500"^^xsd:string ;
    crs-onto:hasName "Drugs, Society, and Behavior"^^xsd:string ;
    crs-onto:hasRequiredPrerequisite crs-courses:crs000892 ;
